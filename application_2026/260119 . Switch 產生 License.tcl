set lic_data ""

set SN EC2603000906
set MAC 94EF97DCE154

set ::LIC_tmp ${SN}_${MAC}_temp.lic
puts $lic_fd [encoding convertfrom utf-8 [base64::decode $lic_data]]
close $lic_fd

set lictmp_fd [open ./extapp/FTP_TFTP_AUTO/ec_license/$::LIC_tmp r+]

set ::LIC [_f_vini_profilerd SFIS SN]_[_f_vini_profilerd SFIS MAC].lic
set licformal_fd [open ./extapp/FTP_TFTP_AUTO/ec_license/$::LIC w+]

while { ! [eof $lictmp_fd]} {
		if {[gets $lictmp_fd line] > 0} {
			puts $licformal_fd $line
		}
}

close $lictmp_fd
close $licformal_fd
